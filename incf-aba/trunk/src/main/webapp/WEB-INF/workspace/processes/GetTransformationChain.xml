<?xml version="1.0" encoding="UTF-8"?>
<ProcessDefinition 
    configVersion="3.0.0" 
    processVersion="1.0.0" 
    storeSupported="true" 
    statusSupported="false" 
    xmlns="http://www.deegree.org/processes/java" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.deegree.org/processes/java http://schemas.deegree.org/processes/java/3.0.0/java.xsd"> 
  <Identifier>GetTransformationChain</Identifier>
  <JavaClass>org.incf.aba.atlas.process.GetTransformationChain</JavaClass>
  <Title>GetTransformation Chain</Title>
  <Abstract>
    Gets the transformation chain between the two SRS (Spatial Reference System) names specified in the request.
  </Abstract>
  
  <InputParameters>
    <LiteralInput>
      <Identifier>transformationCode</Identifier>
      <Title>Transformation Code</Title>
      <Abstract>Transformation code</Abstract>
      <AllowedValues>
        <Value>Mouse_ABAvoxel_1.0_To_Mouse_AGEA_1.0_v1.0</Value>
        <Value>Mouse_AGEA_1.0_To_Mouse_ABAvoxel_1.0_v1.0</Value>
        <Value>Mouse_WHS_0.9_To_Mouse_AGEA_1.0_v1.0</Value>
        <Value>Mouse_AGEA_1.0_To_Mouse_WHS_0.9_v1.0</Value>
        <Value>Mouse_WHS_0.9_To_Mouse_WHS_1.0_v1.0</Value>
        <Value>Mouse_WHS_1.0_To_Mouse_WHS_0.9_v1.0</Value>
        <Value>Mouse_ABAreference_1.0_To_Mouse_ABAvoxel_1.0_v1.0</Value>
        <Value>Mouse_ABAvoxel_1.0_To_Mouse_ABAreference_1.0_v1.0</Value>
        <Value>Mouse_Paxinos_1.0_To_Mouse_WHS_0.9_v1.0</Value>
        <Value>Mouse_WHS_0.9_To_Mouse_Paxinos_1.0_v1.0</Value>
      </AllowedValues>
    </LiteralInput>
  </InputParameters>
  
  <OutputParameters>
    <ComplexOutput>
      <Identifier>GetTransformationChainOutput</Identifier>
      <Title>GetTransformationChain Output</Title>
      <Abstract>This request gets the transformation chain between the two SRS (Spatial Reference System) names specified in the request</Abstract>
      <DefaultFormat mimeType="application/vnd.incf.waxml" encoding="UTF-8"
          schema="http://incf-dai.googlecode.com/svn/waxml/trunk/AtlasXmlBeans2/src/main/xsd/WaxMlSchema/CoordinateChainTransformationResponses.xsd"/>
    </ComplexOutput>
  </OutputParameters>
  
</ProcessDefinition>
