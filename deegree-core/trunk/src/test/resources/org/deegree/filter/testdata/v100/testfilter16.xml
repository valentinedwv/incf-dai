<Filter xmlns="http://www.opengis.net/ogc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.opengis.net/ogc http://schemas.opengis.net/filter/1.0.0/filter.xsd">
	<And>
		<PropertyIsGreaterThan>
			<PropertyName>Person/Age</PropertyName>
			<Literal>50</Literal>
		</PropertyIsGreaterThan>
		<PropertyIsEqualTo>
			<PropertyName>Person/Address/City</PropertyName>
			<Literal>Toronto</Literal>
		</PropertyIsEqualTo>
	</And>
</Filter>