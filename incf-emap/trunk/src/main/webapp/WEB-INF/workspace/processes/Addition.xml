      <Title>First Addend</Title>
      <Abstract>First addend.</Abstract>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#double">double</DataType>
    </LiteralInput>
    <LiteralInput>
      <Identifier>b</Identifier>
      <Title>First Addend</Title>
      <Abstract>First addend.</Abstract>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#double">double</DataType>
    </LiteralInput>
  </InputParameters>
  
  <OutputParameters>
    <LiteralOutput>
      <Identifier>sum</Identifier>
      <Title>Sum</Title>
      <Abstract>The sum of the two input addends.</Abstract>
      <DataType reference="http://www.w3.org/TR/xmlschema-2/#double">double</DataType>
    </LiteralOutput>
  </OutputParameters>
  
